#!/usr/bin/peno

#examples:
psexec \\192.168.50.80 -u offensive -p security ipconfig
psexec -i \myComputer cmd /c "systeminfo"
psexec -i \myComputer -u username -p password cmd

#connecting to DC
PS C:\Tools\active_directory> .\PsExec.exe /accepteula  \\dc01 cmd.exe

