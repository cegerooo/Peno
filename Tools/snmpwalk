#!/usr/bin/peno

snmpwalk -c public -v1 -t 10 10.11.1.14					//enum entire MIB tree
snmpwalk -c public -v1 10.11.1.14 1.3.6.1.4.1.77.1.2.25			//enum windows users
snmpwalk -c public -v1 10.11.1.73 1.3.6.1.2.1.25.4.2.1.2		//enumerate Windows processes
snmpwalk -c public -v1 10.11.1.14 1.3.6.1.2.1.6.13.1.3			//enumerate open TCP ports
snmpwalk -c public -v1 10.11.1.50 1.3.6.1.2.1.25.6.3.1.2		//enumerate installed software

